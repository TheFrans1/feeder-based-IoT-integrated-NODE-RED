    {
        "id": "2448f8e139c7d258",
        "type": "function",
        "z": "c1459de4486b7052",
        "name": "Validasi Akhir",
        "func": "const waktu1 = flow.get('waktu1') || {};\nconst waktu2 = flow.get('waktu2') || {};\nconst durasi = flow.get('durasi');\n\n// Validasi kelengkapan data\nif (!waktu1.payload || !waktu2.payload || !durasi) {\n    return [{ \n        payload: \"ERROR, Pastikan Waktu 1,2 terisi & durasi dipilih\",\n        topic: \"error\"\n    }, null];\n}\n\n// Validasi format waktu\nif (!waktu1.valid || !waktu2.valid) {\n    return [{\n        payload: \"ERROR, Format waktu tidak valid!\",\n        topic: \"error\"\n    }, null];\n}\n\n// Buat dua pesan: satu untuk status, satu untuk MQTT\nconst pesanStatus = `Sukses mengatur Waktu 1 = ${waktu1.payload}, Waktu 2 = ${waktu2.payload}, Durasi: ${durasi} s`;\nconst pesanMQTT = {\n    waktu1: waktu1.payload,\n    waktu2: waktu2.payload,\n    durasi: parseInt(durasi)\n};\n\nreturn [\n    { payload: pesanStatus, topic: \"sukses\" },\n    { payload: JSON.stringify(pesanMQTT), topic: \"data/waktu\" }\n];",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
