    {
        "id": "e9d2f3a09ec0c4dc",
        "type": "function",
        "z": "c1459de4486b7052",
        "name": "Status Validasi",
        "func": "// Update status terbaru\nif (msg.topic === 'waktu1') {\n    flow.set('waktu1', msg);\n} else if (msg.topic === 'waktu2') {\n    flow.set('waktu2', msg);\n} else if (msg.topic === 'durasi') {\n    flow.set('durasi', msg.payload); // TAMBAHKAN INI\n}\n\nconst waktu1 = flow.get('waktu1') || {};\nconst waktu2 = flow.get('waktu2') || {};\nconst durasi = flow.get('durasi'); // AMBIL DARI FLOW\n\n// Cek status semua\nif (waktu1.valid && waktu2.valid && durasi) {\n    // Format pesan baru tanpa JSON.stringify\n    const pesan = `Waktu 1 = ${waktu1.payload}, Waktu 2 = ${waktu2.payload}, Durasi: ${durasi} s`;\n    return { \n        payload: pesan, \n        valid: true \n    };\n} else {\n    return { \n        payload: \"Menunggu data lengkap...\", \n        valid: false \n    };\n}",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
